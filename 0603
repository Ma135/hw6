# include <Rcpp .h>
// [[ Rcpp :: plugins (" cpp11 ")]]

Rcpp::cppFunction('
RObject perms(IntegerVector x) {
IntegerVector out = Rcpp::clone(x);
if (!std::next_permutation(out.begin(), out.end())) return R_NilValue;
return out;
}')

