# include <Rcpp .h>
# include <vector >
// [[ Rcpp :: plugins (" cpp11 ")]]
List simplify2array (List input) {
int n = input.size();
List out (n);
for(int i=0; i<n; ++i) {
out[i] = input[i];
}
return out;
}


//simplify2array(list(1:3,4:6,7:9))
//simplify2array(list(1:3,4:5,6:10)) - can not simplify
//simplify2array(list(1,2,3)) - resuly atomic vector
